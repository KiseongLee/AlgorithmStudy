    "import itertools\n",
    "n = int(input())\n",
    "k = int(input())\n",
    "\n",
    "c=[]\n",
    "for i in range(n):\n",
    "    c.append(int(input()))\n",
    "    \n",
    " \n",
    "res = set()\n",
    "for i in list(itertools.permutations(c, k)):\n",
    "\n",
    "    res.add(''.join(list(map(str,i))))\n",
    "\n",
    "print(len(res))"
   ]
  }
